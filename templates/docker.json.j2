{
    {% if docker_registry_mirror | length > 0 %}
    {% for mirror in docker_registry_mirror %}
    "registry-mirrors": [
        "{{ mirror.host }}",
    ],
    {% endfor %}
    {% endif %}
    {% if docker_private_repo_auth_host | length > 0 %}
    {% for regestry in docker_private_repo_auth_host  %}
    insecure-registries": [
    "{{ regestry.host }}",
    ],
    {% endfor %}
    {% endif %}
}
